{
	"name": "roach",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"drizzle": "npx drizzle-kit push --config=drizzle-dev.config.ts",
		"drizzle:prod": "npx drizzle-kit push --config=drizzle-prod.config.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/pg": "^8.15.5",
		"drizzle-kit": "^0.31.4",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tsx": "^4.20.5",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@auth/drizzle-adapter": "^1.10.0",
		"@auth/sveltekit": "^1.10.0",
		"dotenv": "^17.2.2",
		"drizzle-orm": "^0.44.5",
		"drizzle-zod": "^0.8.3",
		"lucide-svelte": "^0.544.0",
		"pg": "^8.16.3",
